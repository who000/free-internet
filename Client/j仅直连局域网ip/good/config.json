//Web+Tls+caddy;Client
//仅仅直连局域网ip;剩余全部走代理;
{
	//日志配置;
	"log": {
		"loglevel": "warning",
		"access": "/var/log/v2ray/access.log",
		"error": "/var/log/v2ray/error.log"
	},
	//本地代理配置;
  "inbounds": [
    {
      "port": 2018,			
      "listen": "127.0.0.1",
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",			//传输协议;
      "settings": {
        "vnext": [
          {
            "address": "free2aa.tk",	//TLS域名;
            "port": 443,			
            "users": [
              {
                "id": "f8fefa85-87fd-45d8-777f-9c7a7769b299",	//主ID;
                "alterId": 108									//额外ID;
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "path": "/ray"
        }
      },
	  //TCP多路复用;
	  "mux": {
		"enabled": true,
		"concurrency": 8
	  }
    },
	{
		"protocol": "freedom",
		"settings": {},
		"tag" : "direct"
	}
  ],
  //路由配置;
  "routing": {
	"domainStrategy": "IPOnDemand",
	"rules": [
		{
			"type": "field",
			"outboundTag": "direct",
			"ip": [
				//私有ip,路由器等;
				"geoip:private"								
			]
		}
	]
  }
}
