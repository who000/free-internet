//Web+Tls+caddy;Client
//仅仅直连局域网ip;剩余全部走代理;
{
	//日志配置;
	"log": {
		"loglevel": "warning",
		"access": "/var/log/v2ray/access.log",
		"error": "/var/log/v2ray/error.log"
	},
	//本地代理配置;
  "inbound": {
	"listen": "127.0.0.1",
	"port": 2018,
	"protocol": "socks",
	"settings": {
		"auth": "noauth",
		"udp": true,
		"ip": "127.0.0.1"
	}
  },
  "outbounds": [
    {
      "protocol": "vmess",			//传输协议;
      "settings": {
        "vnext": [
          {
            "address": "free2aa.tk",	//TLS域名;
            "port": 56566,			
            "users": [
              {
                "id": "9f80d21f-d35b-554f-18e5-a269a0986cf4",	//主ID;
				"level": 1,
                "alterId": 100									//额外ID;
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "kcp"
        },
	  "mux": {
		"enabled": true,
		"concurrency": 8
	  }
    },
	{
		"protocol": "freedom",
		"settings": {},
		"tag" : "direct"
	}
  ],
  "dns": {
	"server": [
		"1.1.1.1",
		"1.0.0.1",
		"8.8.8.8",
		"8.8.4.4",
		"localhost"
	]
  },
  //路由配置;
  "routing": {
	"domainStrategy": "IPOnDemand",
	"rules": [
		{
			"type": "field",
			"outboundTag": "direct",
			"ip": [
				//私有ip,路由器等;
				"geoip:private"								
			]
		}
	]
  }
}
