//V2ray+Websockt+Tls+Mux Client
{
  "log": {
    "loglevel": "warning",
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log"
  },
  "inbounds": [
    {
      "port": 2018,
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "free2aa.tk",
            "port": 443,  
            "users": [
              {
                "id": "f8fefa85-87fd-45d8-777f-9c7a7769b299",
                "alterId": 108
              }
            ]
          }
        ]
      },
	"streamSettings": {
		"network": "ws",
		"security": "tls",
		"wsSettings": {
			"path": "/ray"
		}
	},
	//TCP多路并发复用;
	"mux": {
		"enabled": true,
		"concurrency": 8
	}
	},
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct" 
    }    
  ],
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": ["geosite:cn"] // 中国大陆主流网站的域名
      },
      {
        "type": "field",
        "outboundTag": "direct",
        "ip": [
          "geoip:cn", // 中国大陆的 IP
          "geoip:private" // 私有地址 IP，如路由器等
        ]
      }
    ]
  }
}
